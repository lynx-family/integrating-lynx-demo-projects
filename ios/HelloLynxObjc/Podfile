target 'Hello-Lynx-OC' do
  pod 'Lynx', '3.4.0-rc.0', :subspecs => [
    'Framework',
  ]


  # integrate image-service, log-service, and http-service
  pod 'LynxService', '3.4.0-rc.0', :subspecs => [
      'Image',
      'Log',
      'Http',
  ]

  # ImageService
  pod 'SDWebImage','5.15.5'
  pod 'SDWebImageWebPCoder', '0.11.0'

  # XElement
  pod 'XElement', '3.4.0-rc.0'
end